{
  "name": "llms-unplugged",
  "version": "1.0.0",
  "description": "Static website for LLMs Unplugged teaching resources",
  "repository": {
    "type": "git",
    "url": "https://github.com/ANUcybernetics/llms-unplugged"
  },
  "homepage": "https://www.llmsunplugged.org",
  "author": "ANU School of Cybernetics",
  "license": "MIT",
  "scripts": {
    "dev": "npm run build:slides && eleventy --serve",
    "build": "eleventy && (test -f .llms-generated/feed.xml && cp .llms-generated/feed.xml _site/feed.xml || true) && npm run build:slides",
    "build:slides": "cd slides && npm install && npm run build && cp -r dist ../_site/slides",
    "preview": "eleventy --serve",
    "pretest": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "lighthouse": "unlighthouse --site http://localhost:8080",
    "lighthouse:ci": "npm run build && unlighthouse-ci --site ./_site"
  },
  "keywords": [],
  "type": "module",
  "dependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-rss": "^2.0.4",
    "@11ty/eleventy-plugin-vite": "^7.0.0",
    "@plausible-analytics/tracker": "^0.4.4",
    "@tailwindcss/typography": "^0.5.19",
    "gray-matter": "^4.0.3",
    "markdown-it-footnote": "^4.0.0",
    "vite-plugin-static-copy": "^3.1.4"
  },
  "devDependencies": {
    "@photogabble/eleventy-plugin-interlinker": "^1.1.2",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/vite": "^4.1.16",
    "@vitejs/plugin-basic-ssl": "^2.1.0",
    "markdown-it": "^14.1.0",
    "unlighthouse": "^0.17.4",
    "vite": "^7.1.12",
    "vitest": "^4.0.3"
  }
}
